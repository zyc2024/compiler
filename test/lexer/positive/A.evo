
int inc(int x) {
    return x+1;
}

int zero(void) { //illegal syntax but this is lexing
    return 0;
}

int compose(int n) {
    if (n == 0) {
        return zero();
    } else {
        return inc(compose(n-1));
    }
}

int main(char[][] args) {
    if (args[0][0] != '-') {
        return compose(0);
